{
  "name": "@bot/bot",
  "version": "1.1.1",
  "main": "dist/index.js",
  "author": "Anas Mazouni",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn src/index.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "db:pull": "npx prisma db pull",
    "db:gen": "npx prisma generate",
    "postinstall": "npm run db:gen",
    "prebuild": "rm -rf dist && mkdir dist",
    "genversion": "genversion --es6 --semi src/version.ts"
  },
  "devDependencies": {
    "@types/ascii-art": "^1.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "genversion": "^3.1.1",
    "prettier": "^2.8.4",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.3",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@bot/language": "workspace:^1.1.0",
    "@prisma/client": "^4.11.0",
    "@sentry/node": "^7.44.2",
    "@sentry/tracing": "^7.44.2",
    "ascii-art": "^2.8.5",
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "glob": "^9.3.0",
    "lodash": "^4.17.21",
    "node-graceful-shutdown": "^1.1.5",
    "prisma": "^4.11.0",
    "reflect-metadata": "^0.1.13",
    "tslog": "^4.8.2",
    "twitch-js": "^2.0.0-beta.45"
  }
}
